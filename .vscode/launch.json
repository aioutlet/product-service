{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "üêç Direct",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": ["main:app", "--host", "0.0.0.0", "--port", "1001", "--reload"],
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "justMyCode": false,
      "jinja": true,
      "preLaunchTask": "Kill Port 1001",
      "env": {
        "PYTHONUNBUFFERED": "1",
        "LOG_LEVEL": "debug"
      }
    },
    {
      "name": "üöÄ Dapr",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "main:app",
        "--host", "0.0.0.0",
        "--port", "1001",
        "--reload"
      ],
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "justMyCode": false,
      "jinja": true,
      "preLaunchTask": "Start Dapr Sidecar",
      "postDebugTask": "Stop Dapr Sidecar",
      "env": {
        "PYTHONUNBUFFERED": "1",
        "LOG_LEVEL": "debug",
        "DAPR_HTTP_PORT": "3501",
        "DAPR_GRPC_PORT": "50001"
      }
    }
  ],
  "compounds": []
}
